    "devDependencies": {
        "@ai-sdk/mcp": "^1.0.21",
        "@ai-sdk/openai": "^3.0.30",
        "@types/ws": "^8.18.1",
        "@types/yargs": "^17.0.33",
        "ai": "^6.0.97",
        "tsup": "^8.5.1",
        "tsx": "^4.19.0",
        "typescript": "^5.9.3",
        "vitest": "^3.0.0"
